{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "NodeJs API with Typescript using the best practices",
  "main": "index.js",
  "repository": "https://github.com/jgsn13/clean-node-api",
  "author": "Joaquim Greg√≥rio da Silva Neto <joaquim.ibi@gmail.com>",
  "license": "GPL-2.0",
  "private": false,
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "yarn test --watch -c jest-unit.config.ts",
    "test:integration": "yarn test --watch -c jest-integration.config.ts",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.9.0",
    "sucrase": "^3.25.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
